{
  "version": 2,
  "framework": "nextjs",
  "builds": [
    {
      "src": "apps/knockdog/package.json",
      "use": "@vercel/next"
    }
  ],
  "buildCommand": "pnpm build",
  "installCommand": "pnpm install",
  "outputDirectory": "apps/knockdog/.next",
  "env": {
    "NEXT_PUBLIC_API_BASE": "https://api.knockdog.net"
  }
}
