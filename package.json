{
  "name": "front_v2",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "type-check": "turbo type-check",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "prepare": "husky",
    "commit": "cz",
    "extract-icons": "pnpm run --filter @knockdog/icons extract-icons"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,md,yml,yaml}": "prettier --write",
    "*.{ts,tsx}": "eslint"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "packageManager": "pnpm@8.15.6",
  "dependencies": {
    "@types/react-lottie": "^1.2.10",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-lottie": "^1.2.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@svgr/webpack": "^8.1.0",
    "@types/node": "^22.15.18",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.5",
    "commitizen": "^4.3.1",
    "cz-git": "^1.10.1",
    "eslint": "^9.27.0",
    "eslint-config-expo": "~9.2.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "husky": "^9.1.6",
    "lint-staged": "^16.0.0",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "turbo": "^2.5.4",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=18"
  }
}
