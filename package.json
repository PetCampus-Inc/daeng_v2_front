{
  "name": "front_v2",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "type-check": "turbo type-check",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "prepare": "husky",
    "commit": "cz"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "./apps/member/**/*.{ts,tsx}": "bash -c 'pnpm run --filter member type-check'",
    "./packages/ui/**/*.{ts,tsx}": "bash -c 'pnpm run --filter @knockdog/ui type-check'"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "packageManager": "pnpm@8.15.6",
  "dependencies": {
    "@types/react-lottie": "^1.2.10",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "next": "^14.2.15",
    "react": "^18.2.0",
    "react-dom": "^18.3.1",
    "react-lottie": "^1.2.4",
    "tailwind-merge": "^2.5.4",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@svgr/webpack": "^8.1.0",
    "@types/node": "^20.11.24",
    "@types/react": "^18.2.61",
    "@types/react-dom": "^18.2.19",
    "autoprefixer": "^10.4.20",
    "commitizen": "^4.3.1",
    "cz-git": "^1.10.1",
    "eslint": "^8.56.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "tailwindcss": "^3.4.14",
    "turbo": "^2.1.3",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": ">=18"
  }
}
