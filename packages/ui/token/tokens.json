{
  "core": {
    "opacity": {
      "low": {
        "$type": "opacity",
        "$value": "10%"
      },
      "md": {
        "$type": "opacity",
        "$value": "50%"
      },
      "high": {
        "$type": "opacity",
        "$value": "90%"
      }
    },
    "Heading": {
      "$type": "fontFamilies",
      "$value": "SUIT"
    },
    "line-height": {
      "h1": {
        "$type": "lineHeights",
        "$value": "2.125rem"
      }
    },
    "font-size": {
      "h1": {
        "$type": "fontSizes",
        "$value": "24px"
      }
    },
    "spacing": {
      "x0_5": {
        "$type": "dimension",
        "$value": "2px"
      },
      "x1": {
        "$type": "dimension",
        "$value": "4px"
      },
      "x2": {
        "$type": "dimension",
        "$value": "8px"
      }
    },
    "radius": {
      "r1": {
        "$type": "dimension",
        "$value": "4px"
      }
    },
    "font-weight": {
      "extrabold": {
        "$type": "fontWeights",
        "$value": "800"
      },
      "bold": {
        "$type": "fontWeights",
        "$value": "700"
      },
      "semibold": {
        "$type": "fontWeights",
        "$value": "600"
      },
      "medium": {
        "$type": "fontWeights",
        "$value": "500"
      },
      "regular": {
        "$type": "fontWeights",
        "$value": "400"
      }
    },
    "letter-spacing": {
      "tighter": {
        "$type": "letterSpacing",
        "$value": "-2%"
      },
      "tight": {
        "$type": "letterSpacing",
        "$value": "-1%"
      },
      "normal": {
        "$type": "letterSpacing",
        "$value": "0%"
      }
    },
    "typography": {
      "H1": {
        "$type": "typography",
        "$value": {
          "fontFamily": "SUIT",
          "fontWeight": "{font-weight.extrabold}",
          "fontSize": "{font-size.h1}",
          "lineHeight": "{line-height.h1}",
          "letterSpacing": "{letter-spacing.tighter}"
        }
      }
    },
    "color": {
      "primitive": {
        "orange-50": {
          "$type": "color",
          "$value": "#FFF7EC"
        },
        "orange-100": {
          "$type": "color",
          "$value": "#FFECD3"
        },
        "orange-200": {
          "$type": "color",
          "$value": "#FFD6A6"
        },
        "orange-300": {
          "$type": "color",
          "$value": "#FFB86E"
        }
      },
      "fill": {
        "primary-50": {
          "$type": "color",
          "$value": "{color.primitive.orange-50}"
        }
      }
    }
  },
  "$themes": [],
  "$metadata": {
    "tokenSetOrder": [
      "core"
    ]
  }
}