{
  "core": {
    "opacity": {
      "low": {
        "$type": "opacity",
        "$value": "10%"
      },
      "md": {
        "$type": "opacity",
        "$value": "50%"
      },
      "high": {
        "$type": "opacity",
        "$value": "90%"
      }
    },
    "font-family": {
      "suit": {
        "$type": "fontFamilies",
        "$value": "SUIT"
      }
    },
    "line-height": {
      "Heading": {
        "$type": "lineHeights",
        "$value": "2.125rem"
      }
    },
    "font-size": {
      "h1": {
        "$type": "fontSizes",
        "$value": "24px"
      }
    },
    "spacing": {
      "x0_5": {
        "$type": "dimension",
        "$value": "2px"
      },
      "x1": {
        "$type": "dimension",
        "$value": "4px"
      },
      "x2": {
        "$type": "dimension",
        "$value": "{spacing.x1} * {spacing.x0_5}"
      }
    },
    "radius": {
      "r1": {
        "$type": "dimension",
        "$value": "4px"
      }
    },
    "h1ExtraBold": {
      "$type": "fontWeights",
      "$value": "800"
    },
    "letter-spacing": {
      "ls1": {
        "$type": "letterSpacing",
        "$value": "-2%"
      }
    },
    "typography": {
      "h1": {
        "$type": "typography",
        "$value": {
          "fontFamily": "{font-family.suit}",
          "fontWeight": "{h1ExtraBold}",
          "fontSize": "{font-size.h1}",
          "lineHeight": "{line-height.Heading}",
          "letterSpacing": "{letter-spacing.ls1}"
        }
      }
    },
    "color": {
      "primitive": {
        "orange": {
          "50": {
            "$type": "color",
            "$value": "#FFF7EC"
          },
          "100": {
            "$type": "color",
            "$value": "#FFECD3"
          },
          "200": {
            "$type": "color",
            "$value": "#FFD6A6"
          },
          "300": {
            "$type": "color",
            "$value": "#FFB86E"
          }
        }
      },
      "fill": {
        "primary": {
          "50": {
            "$type": "color",
            "$value": "{color.primitive.orange.50}"
          }
        }
      }
    }
  },
  "$themes": [],
  "$metadata": {
    "tokenSetOrder": [
      "core"
    ]
  }
}