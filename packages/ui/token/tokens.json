{
  "core": {
    "borderRadius": {
      "sm": {
        "$type": "borderRadius",
        "$value": "4"
      },
      "lg": {
        "$type": "borderRadius",
        "$value": "8"
      },
      "xl": {
        "$type": "borderRadius",
        "$value": "16"
      },
      "multi-value": {
        "$type": "borderRadius",
        "$value": "{borderRadius.sm} {borderRadius.lg}",
        "$description": "You can have multiple values in a single radius token. Read more on these: https://docs.tokens.studio/available-tokens/border-radius-tokens#single--multiple-values"
      }
    },
    "opacity": {
      "low": {
        "$type": "opacity",
        "$value": "10%"
      },
      "md": {
        "$type": "opacity",
        "$value": "50%"
      },
      "high": {
        "$type": "opacity",
        "$value": "90%"
      }
    },
    "fontFamilies": {
      "suit": {
        "$type": "fontFamilies",
        "$value": "SUIT"
      }
    },
    "lineHeights": {
      "Heading": {
        "$type": "lineHeights",
        "$value": "2.125rem"
      },
      "body": {
        "$type": "lineHeights",
        "$value": "140%"
      }
    },
    "paragraphSpacing": {
      "h1": {
        "$type": "paragraphSpacing",
        "$value": "32"
      }
    },
    "fontSizes": {
      "h1": {
        "$type": "fontSizes",
        "$value": "24px"
      }
    },
    "Spacing": {
      "spacing-s": {
        "$type": "dimension",
        "$value": "2px"
      },
      "spacing-01": {
        "$type": "dimension",
        "$value": "4px"
      }
    },
    "border-radius": {
      "radius-01": {
        "$type": "dimension",
        "$value": "4px"
      }
    },
    "h1ExtraBold": {
      "$type": "fontWeights",
      "$value": "800"
    },
    "LetterSpacing": {
      "ls1": {
        "$type": "letterSpacing",
        "$value": "-2%"
      }
    },
    "Typography": {
      "h1": {
        "$type": "typography",
        "$value": {
          "fontFamily": "{fontFamilies.suit}",
          "fontWeight": "{h1ExtraBold}",
          "fontSize": "{fontSizes.h1}",
          "lineHeight": "{lineHeights.Heading}",
          "letterSpacing": "{LetterSpacing.ls1}"
        }
      }
    },
    "color": {
      "primitive": {
        "orange": {
          "50": {
            "$type": "color",
            "$value": "#FFF7EC"
          },
          "100": {
            "$type": "color",
            "$value": "#FFECD3"
          },
          "200": {
            "$type": "color",
            "$value": "#FFD6A6"
          },
          "300": {
            "$type": "color",
            "$value": "#FFB86E"
          }
        }
      },
      "fill": {
        "primary": {
          "50": {
            "$type": "color",
            "$value": "{color.primitive.orange.50}"
          }
        }
      }
    }
  },
  "$themes": [],
  "$metadata": {
    "tokenSetOrder": [
      "core"
    ]
  }
}